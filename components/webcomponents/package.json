  "scripts": {
    "build": "pnpm run prettify:src && pnpm run prettify:readme && ts-node -O '{ \"module\": \"commonjs\" }' build.ts",
    "build:ci": "pnpm run prettify:ci:src && pnpm run prettify:ci:readme && ts-node -O '{ \"module\": \"commonjs\" }' build.ts",
    "prettify:ci:src": "prettier --check ./src/*",
    "prettify:ci:readme": "prettier --check ./README.md",
    "prettify:src": "prettier --write ./src/*",
    "prettify:readme": "prettier --write ./README.md",
    "prepack": "pnpm run build"
  },
